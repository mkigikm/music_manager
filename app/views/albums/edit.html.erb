<% content_for :title do %>
<h1>Edit <%= @album.title %></h1>
<% end %>

<%= render(
  partial: "shared/errors",
  locals: {errors: @album.errors.full_messages}
)%>

<%= render partial: "form", locals: {album: @album, bands: @bands} %>

<p>
  <a href="<%= album_url(@album) %>">Back to <%= @album.title %></a>
</p>
